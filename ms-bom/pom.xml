<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.6.7</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>
    <groupId>com.msop.platform</groupId>
    <artifactId>ms-bom</artifactId>
    <version>${ms.tool.version}</version>
    <packaging>pom</packaging>
    <properties>
        <ms.tool.version>0.2.1-SNAPSHOT</ms.tool.version>
        <guava.version>20.0</guava.version>
    </properties>
    <dependencyManagement>
        <dependencies>
            <!-- MSToolbox 核心依赖版本管理 -->
            <!-- 自动创建配置 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-auto</artifactId>
                <version>${ms.tool.version}</version>
                <scope>provided</scope>
            </dependency>
            <!-- Spring boot 工具 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-boot</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>
            <!-- Spring Cloud 工具 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-cloud</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>
            <!-- 上下文 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-context</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>
            <!-- 数据库 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-db</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>
            <!-- 启动配置 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-launch</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>
            <!-- 日志处理 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-log4j2</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>
            <!-- 对象存储工具 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-oss</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>
            <!-- 安全工具 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-secure</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>
            <!-- 短信工具 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-sms</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>
            <!-- 测试工具 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-test</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>
            <!-- 通用工具模块 -->
            <dependency>
                <groupId>com.msop</groupId>
                <artifactId>ms-core-tool</artifactId>
                <version>${ms.tool.version}</version>
            </dependency>


            <!--  -->
            <dependency>
                <groupId>com.fasterxml.jackson.datatype</groupId>
                <artifactId>jackson-datatype-jsr310</artifactId>
            </dependency>
            <dependency>
                <groupId>com.google.guava</groupId>
                <artifactId>guava</artifactId>
                <version>${guava.version}</version>
            </dependency>
            <!-- Okhttp -->
            <dependency>
                <groupId>com.squareup.okhttp3</groupId>
                <artifactId>okhttp</artifactId>
            </dependency>
            <!-- Swagger -->
            <dependency>
                <groupId>io.springfox</groupId>
                <artifactId>springfox-swagger2</artifactId>
                <version>${swagger.version}</version>
                <exclusions>
                    <exclusion>
                        <groupId>io.swagger</groupId>
                        <artifactId>swagger-models</artifactId>
                    </exclusion>
                </exclusions>
            </dependency>
            <dependency>
                <groupId>io.swagger</groupId>
                <artifactId>swagger-models</artifactId>
                <version>${swagger.models.version}</version>
            </dependency>
            <!-- protostuff -->
            <dependency>
                <groupId>io.protostuff</groupId>
                <artifactId>protostuff-core</artifactId>
                <version>${protostuff.version}</version>
            </dependency>
            <dependency>
                <groupId>io.protostuff</groupId>
                <artifactId>protostuff-runtime</artifactId>
                <version>${protostuff.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>
</project>
